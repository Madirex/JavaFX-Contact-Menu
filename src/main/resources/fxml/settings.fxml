<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.madirex.javafxmenu.controller.SettingsController">
   <Label text="%title.settings" textAlignment="CENTER" minWidth="400">
      <font>
         <Font name="Arial" size="18.0"/>
      </font>
      <VBox.margin>
         <Insets bottom="20.0" left="50.0" right="50.0" top="50.0"/>
      </VBox.margin>
   </Label>
   <VBox fx:id="content">
      <VBox.margin>
         <Insets bottom="20.0" left="50.0" right="50.0" top="10.0"/>
      </VBox.margin>
   </VBox>
</VBox>
